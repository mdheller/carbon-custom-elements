    "format:diff": "prettier --check \"**/*.{css,js,md,scss}\"",
    "format:staged": "prettier --write",
    "lint": "yarn lint:src && yarn lint:dist",
    "lint:src": "yarn lint:license:src && yarn lint:scripts",
